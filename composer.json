{
    "name": "highvertical/widget-package",
    "description": "A simple and flexible widget package for Laravel",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Highvertical",
            "email": "highverticaltechnologies@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Highvertical\\WidgetPackage\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Highvertical\\WidgetPackage\\Providers\\WidgetServiceProvider"
            ]
        }
    }
}
